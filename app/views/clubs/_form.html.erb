<%= form_for @club do |f| %>
<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, :required => true, :autofocus => true, :class => 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :country%>
  <%= f.text_field :country, :class => 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :city %>
  <%= f.text_field :city, :class => 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :founded %>
  <%= f.text_field :founded, :class => 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :image %>
  <%= f.text_field :image, :class => 'form-control' %>
</div>

<%= f.submit :class => 'btn btn-success btn-lg' %>
<% end %>
